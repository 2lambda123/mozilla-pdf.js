{
  "name": "uriloader@pdf.js",
  "version": "0.1",
  "description": "Read PDF Document",
  "permissions": [
    "webRequest", "webRequestBlocking",
    "http://*/*.pdf",
    "file:///*/*.pdf"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*.pdf",
        "file:///*/*.pdf"
      ],
      "js": ["insertPdfIframe.js"],
      "run_at": "document_start"
    }
  ]
}
